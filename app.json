{
  "expo": {
    "name": "my-recipe-box",
    "slug": "my-recipe-box",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "runtimeVersion": "sdk-51.0.38",
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/splash-light.png",
        "resizeMode": "cover",
        "backgroundColor": "#FF7900",
        "dark": {
          "image": "./assets/splash-dark.png",
          "resizeMode": "cover",
          "backgroundColor": "#803C00"
        }
      }
    },
    "android": {
      "splash": {
        "image": "./assets/splash-light.png",
        "resizeMode": "cover",
        "backgroundColor": "#FF7900",
        "dark": {
          "image": "./assets/splash-dark.png",
          "resizeMode": "cover",
          "backgroundColor": "#803C00"
        }
      },
      "package": "com.wisso124214.myrecipebox"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-font",
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "23ff6676-2bff-430d-8b8b-ed6003ac4952"
      }
    },
    "owner": "wisso124214"
  }
}
